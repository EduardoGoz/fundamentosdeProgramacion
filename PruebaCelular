

package celular;

public class Celular {


    public static void main(String[] args) 
    {
        Celular Nokia = new Celular (8);
        System.out.println(Nokia);
         
        Celular Samsung = new Celular (10,true);
        System.out.println(Samsung);
         
        Celular Sony = new Celular(10, true, true);
        System.out.println(Sony);
        
        Celular motorola = new Celular(5,true,true,false);
        System.out.println( motorola);
        
        Celular alcatel = new Celular(5,true,true,false,"alcatel");
        System.out.println( alcatel);
        
        Celular LG = new Celular(90, true, true, false, "LG", 4.7);
        System.out.println(LG);
        
        Celular Iphone = new Celular(1000,true,true,false,"APPLE",5.5,true);
        //System.out.println(Iphone);
        
        Iphone.setEctopollo(4.5);
        Iphone.setMarca("Sony");
        Iphone.setMemoria(10);
        Iphone.setlampara(true);
        Iphone.setReproductor(false);
        Iphone.setTecladofisico(true);
        
        //System.out.println(Iphone);
        System.out.println("DATOS DEL CELULAR\n");
        System.out.printf("Marca: %s\n", Iphone.getMarca());
        System.out.printf("Memoria: %d\n", Iphone.getMemoria());
        
        
        
        
    }
    
}
